{
  "manifest_version": 3,
  "name": "SyncMark",
  "description": "A Chrome extension that syncs and manages your bookmarks",
  "version": "0.1.0",
  "author": "vikramjit360@gmail.com",
  "permissions": [
    "bookmarks"
  ],
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "action": {
    "default_title": "Syncmark",
    "default_popup": "popup.html"
  }
}